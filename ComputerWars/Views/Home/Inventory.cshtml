
@{
    ViewBag.Title = "Inventory";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="inventory">
    <p>Processors: @Model.Inventory["Processors"]</p>
    <p>Graphics Cards: @Model.Inventory["Graphics Cards"]</p>
